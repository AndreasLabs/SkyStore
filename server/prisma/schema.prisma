// This is your main Prisma schema file
// Models are now organized in separate files in the models/ directory:
// - models/auth.prisma    - User and Session models
// - models/asset.prisma   - Asset model
// - models/flight.prisma  - Flight model

generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["prismaSchemaFolder"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}
